#!/usr/bin/env bash
# vim: set ft=sh:

echo "${HOSTNAME:-<unset>} $(date +"%Y-%m-%d %H:%M:%S") $$ $PPID zsh/zsh_env" >> "$HOME/.dot_log"

# Initialise DOTFILES
# shellcheck source=/home/sean/.dot
[ -f "$HOME/.dot" ] && . "$HOME/.dot"

# Pick up common functions
if [[ -d $DOTFILES/common/functions ]]
then
	fpath+=($DOTFILES/common/functions)
fi
