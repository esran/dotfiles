# shellcheck shell=bash
# shellcheck disable=SC1090,SC1091
# vim: se syn=sh:

# Initialise DOTFILES
[[ -f $HOME/.dot ]] && source "$HOME/.dot"

echo "$HOSTNAME $(date +"%Y-%m-%d %H:%M:%S") $$ $PPID zsh/zsh_login" >> "$HOME/.dot_log"

# Runs last (after .zprofile -> .zshrc) for each shell created in the gnome-session

:
